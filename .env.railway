# Railway deployment environment variables
# These override defaults in production

# Disable database warm-up on startup to prevent timeouts
WARM_DB_ON_STARTUP=false

# Enable caching for better performance
ENABLE_CACHE=true
CACHE_TTL=300

# Connection pool settings
MAX_BATCH_SIZE=50
CONCURRENT_EXTRACTIONS=5

# Enable rate limiting for protection
ENABLE_RATE_LIMIT=true

# Disable metrics if not needed (reduces overhead)
ENABLE_METRICS=false